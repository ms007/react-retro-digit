{
  "name": "react-retro-digit",
  "version": "0.0.2",
  "description": "A retro digital number component for React",
  "author": "ms007",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ms007/react-retro-digit.git"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/react-retro-digit.umd.cjs",
  "module": "./dist/react-retro-digit.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/react-retro-digit.js",
      "require": "./dist/react-retro-digit.umd.cjs"
    }
  },
  "sideEffects": false,
  "keywords": [
    "react",
    "digital",
    "digit",
    "clock",
    "42",
    "retro",
    "number"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepublish": "npm run build",
    "publish": "clean-publish"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "clean-publish": "^5.1.0",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.5.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
